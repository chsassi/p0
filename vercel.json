{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.protocolzero.online"
        }
      ],
      "destination": "https://protocolzero.online/$1",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/#about",
      "permanent": true
    },
    {
      "source": "/contacts",
      "destination": "/#contacts",
      "permanent": true
    }
  ]
}